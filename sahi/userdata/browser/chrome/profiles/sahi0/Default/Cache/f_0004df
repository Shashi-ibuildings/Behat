<script>
(function($) {
    $(document).ready(function() {
        var products = [];
        $('figure > a.hotspot[id]').each(function() {
            products.push(this.id);
        });

        $.ajax({
            url: '/ajaxprice',
            data: { p: products },
            success: function(data) {
                $.each(data, function(sku, block) {
                    $('figure > a#'+sku).append(block.content)
                });
            }
        });
    });
})(jQuery);
</script>

