jQuery(document).ready(function() {
    /* Re-write of navigation client-side functionality
     * with JQuery instead of Mootools
     */
	if(jQuery('header a#nav-link, header span#nav-close').attr('href') == 'javascript:;') {
	    jQuery('header a#nav-link, header span#nav-close').click(function(event) {
	    	event.stopPropagation();
	    	event.preventDefault();
	    	
	    	if(!jQuery('header').hasClass('navigation')) {
	    		jQuery('header').addClass("navigation");
	    	}
	    	else {
	    		jQuery('header').removeClass("navigation");
	    	}
	    });
	    
	    jQuery('header nav').mouseleave(function(event) {
	    	event.stopPropagation();
	    	event.preventDefault();
	    	
	    	jQuery('header').removeClass('navigation');
	    	jQuery('header').removeClass('shoppingbag');
	    });
	}
});
// This file is included from every page
var $choosenRegionSelector = 0;

if (!$j) {
  var $j = jQuery.noConflict(); 
}

function saveValue() {
    $choosenRegionSelector = $('choosenRegionSelector').selectedIndex;
}

var current_url = window.location.toString().replace('#','');
if(current_url.indexOf('?') != -1) current_url = current_url.split('?')[0];

if(current_page == undefined) {
	// current_page is sometimes set in template (catalog/product/list.phtml)
	var current_page = current_url.split('/');
	current_page = (current_page[current_page.length-1] == '') ? current_page[current_page.length-2] : current_page[current_page.length-1];
	if(current_page.indexOf('.')) current_page = current_page.split('.')[0];
}

if((window.location == jsBaseUrl) || (window.location == 'http://www.acnestudios.com/')) current_page = 'start';

