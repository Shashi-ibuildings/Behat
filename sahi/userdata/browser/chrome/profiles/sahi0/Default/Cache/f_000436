<script type="text/javascript">
function changeChooseCountrySidebar() {
    var shippingCountryId;
    if (typeof $('choosenRegionSelector') !== 'undefined') {
        shippingCountryId = $('choosenRegionSelector').value;
    }
    var changeCountry = true;	// Originally: confirm('Your shopping bag may be emptied if you change country. Continue?');
    var redirectToken = 'index';

    if (changeCountry) {
        new Ajax.Request('/productmanagement/index/saveItems/', {
            method: 'post',
            parameters: { 'choosenCountryId': shippingCountryId },
            onSuccess: function (transport) {
                var result = transport.responseText.evalJSON().random_nr;

                if (result != '0') {
                    // Country doesn't belong to this store.
                    window.location = ([
                        'http://uat.acnestudios.com/shipping-destination/',
                        '?forceCountry=', shippingCountryId,
                        '&cart=', result,
                        '&redirectToken=', redirectToken
                    ]).join('');
                } else {
                    // Country belongs to this store.
                    window.location = ([
                        '/shipping-destination/?forceCountry=DE&cart=1324079958_&redirectToken=index',
                        '&',
                        'forceCountry=', shippingCountryId,
                        '&redirectToken=', redirectToken
                    ]).join('');
                }
            },
            onFailure: function () {
                console.log('Failure when setting shipping country.');
            }
        });
    } else {
        $('choosenRegionSelector').options[$choosenRegionSelector].selected = true;
    }
}

Event.observe(window, 'load', function() {
    $('new_country').update('Germany ');
    $('country_sellector_sidebar_id', 'country_sellector_txt').invoke('hide');
    $('reload').setStyle({ 'display': 'block' });

    var redirectUrl = 'http://uat.acnestudios.com/';
    setTimeout(function() { window.location = redirectUrl; }, 2500);
});
</script>
